{
  "name": "catalog",
  "version": "1.1.0",
  "description": "API for managing and accessing the Wavlake catalog",
  "main": "index.js",
  "engines": {
    "node": ">=20.17.0 <21"
  },
  "scripts": {
    "test": "jest --testPathIgnorePatterns=\"integration\"",
    "integration": "jest ./tests/integration --detectOpenHandles --forceExit",
    "start": "node dist/index.js",
    "dev": "NODE_ENV=dev concurrently \"npx tsc --watch\" \"nodemon dist/index.js --watch\"",
    "create-env": "printenv > .env",
    "build": "npx tsc",
    "migrate": "knex --knexfile ./db/knexfile.js migrate:latest",
    "rollback": "knex --knexfile ./db/knexfile.js migrate:rollback",
    "pull": "npx prisma db pull",
    "generate": "npx prisma generate"
  },
  "author": "Michael Rhee",
  "license": "MIT",
  "dependencies": {
    "@noble/hashes": "^1.3.2",
    "@podverse/podcast-feed-parser": "https://github.com/joshr4/podcast-feed-parser/tree/add-time-split-support",
    "@prisma/client": "^5.20.0",
    "@sentry/node": "^7.60.0",
    "@types/multer": "^1.4.7",
    "aws-sdk": "^2.1395.0",
    "axios": "^1.6.2",
    "body-parser": "^1.19.0",
    "compression": "1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.1.3",
    "express": "^4.21.0",
    "express-async-handler": "^1.2.0",
    "express-rate-limit": "^7.1.5",
    "firebase-admin": "^11.9.0",
    "helmet": "^3.22.0",
    "jimp": "^1.6.0",
    "knex": "^2.5.1",
    "knex-paginate": "^3.1.4",
    "loglevel": "^1.9.2",
    "multer": "^1.4.5-lts.1",
    "node-fetch": "^2.7.0",
    "node-lame": "^1.3.2",
    "node-vibrant": "^3.2.1-alpha.1",
    "nostr-tools": "^2.5.2",
    "oauth": "^0.10.0",
    "pg": "^8.7.1",
    "podcast-index-api": "^1.1.10",
    "sanitize-html": "^2.13.0",
    "unique-names-generator": "^4.7.1",
    "uuid": "^9.0.1",
    "ws": "^8.17.0"
  },
  "devDependencies": {
    "@babel/core": "^7.23.9",
    "@babel/preset-env": "^7.23.9",
    "@babel/preset-typescript": "^7.22.15",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.4",
    "@types/node": "^20.2.5",
    "babel-jest": "^29.7.0",
    "concurrently": "^8.1.0",
    "jest": "^29.6.4",
    "nodemon": "^2.0.22",
    "prisma": "^5.20.0",
    "typescript": "^5.1.3"
  }
}
